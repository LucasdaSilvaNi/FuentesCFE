@model TiempoEnProcesoUIWeb.Models.BusquedaClienteModel

            @using (Html.BeginForm("BusquedaClientes", "CapturaWeb", FormMethod.Get, new { role = "form", id = "formData", @class = "form-horizontal" }))
            {
                <div class="row">
                    <div class="col-lg-12">
                        <h4 class="page-header"><strong>Captura WEB - Busqueda Cliente</strong></h4>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>                
                
                <div class="form-group">
                    @Html.LabelFor(model => model.id_cliente, new {@Class="col-md-1 control-label"})
                    <div class="col-md-2">
                        @Html.TextBoxFor(model=> model.id_cliente,  new { @Class="form-control", autocomplete="off", autofocus="autofocus" })
                    </div>  
                
                    @Html.LabelFor(model => model.nombre, new {@Class="col-md-1 control-label"})
                    <div class="col-md-4">
                        @Html.TextBoxFor(model=> model.nombre,  new { @Class="form-control", autocomplete="off" })
                    </div> 
                    <button type="submit" class="btn btn-primary">Buscar <i class="fa fa-search"></i></button>
                </div>
                @Html.HiddenFor(model=> model.id_cliente_sel)
                @Html.HiddenFor(model => model.id_oficina);
            }
            
            @Html.Partial("_Clientes", Model.clientes)

            <hr />
            <a id="btnBusquedaClie" class="btn btn-primary" role="button">Seleccionar</a>


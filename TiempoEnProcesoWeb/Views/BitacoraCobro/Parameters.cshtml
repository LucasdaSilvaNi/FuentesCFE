@model TiempoEnProcesoWeb.Models.BitacoraCobroModel

            @using (Html.BeginForm("Generate","BitacoraCobro", FormMethod.Post, new {role="form", id="formData"}))
            {
                <div class="row">
                    <div class="col-lg-10 col-lg-offset-2">
                        <p class="form-control-static text-primary" style="text-align:right">@Session[TiempoEnProcesoHelper.Constantes.S_NOMBREEMP]</p> 
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <div class="row">
                    <div class="col-lg-10 col-lg-offset-1">
                        <h1 class="page-header" >Bitacora de Cobros</h1>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <div class="row">
                    <div class="col-lg-10 col-lg-offset-1">
                        <div class="panel panel-primary">
                            <div class="panel-heading" >
                                Parametros
                            </div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-md-6 ">                                    
                                        <div class="form-group">
                                            @Html.LabelFor(model => model.Imprime)
                                            @Html.CheckBoxFor(model=> model.Imprime)
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @Html.HiddenFor(model => model.EsSocio)
                @Html.HiddenFor(model => model.EsGerente)
                @Html.HiddenFor(model=> model.SocioBitacora)
                @Html.HiddenFor(model=> model.EncargadoBitacora)
                <div>
                    <div class="row">
                        <div class="col-lg-1 col-lg-offset-1">
                            <input type="submit" class="btn btn-success" value="Generar"/>
                        </div>
                    </div>
                </div>
            }

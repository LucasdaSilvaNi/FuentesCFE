@using Microsoft.Reporting.WebForms;
@using MvcReportViewer;
@using TiempoEnProcesoWeb.Models;

@model ReportModel

@{
    ViewBag.Title = "Generate";
}

@Html.MvcReportViewerFluent(string.Format("App_Data/Reports/sumario_facturacion_{0}.rdlc",Model.EsSocio?"socio":"encargado")).ProcessingMode(ProcessingMode.Local).LocalDataSource("pwcia", Model.Data).EventsHandlerType(typeof(SumarioFacturacionSubReportHandler)).Attributes(new { style = "position: absolute; width: 75%; height: 75%; border: none;", }).Method(FormMethod.Post)
                
